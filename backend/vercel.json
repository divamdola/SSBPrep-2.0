{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/my-app/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "frontend/my-app/build" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/my-app/build/index.html"
      }
    ]
  }
  